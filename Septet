#include <stdio.h>

int main() {
    char str[] = "LEAP";
    int i, j, len = strlen(str);

    for (i = 0; i < len; i++) {
        for (j = 0; j < len; j++) {
            if (i == j) {
                printf("%c ", str[i]);
            } else {
                printf("* ");
            }
        }
        printf("\n");
    }

    return 0;
}

