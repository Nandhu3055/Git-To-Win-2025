#include <stdio.h>
#include <string.h>

int main() {
    char str[] ="LEAP";
    int len = strlen(str);

    for (int i = 0; i < len; i++) {
        for (int j = 0; j < len; j++) {
            if (i == j) {
                printf("%c", str[i]); 
            } else {
                printf("*");
            }
        }
        printf("\n");
    }

    return 0;
}
