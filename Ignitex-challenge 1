#include<stdio.h>
#include<conio.h>
int main(){
    char a[5] = {'L','E','A','P'};
    for(int i = 0;i < 4;i++){
        for(int j = 0 ;j < 4;j++){
            if (j == i){
                printf("%c",a[i]);
            }
            else{
            printf("*");
            }
        }
        printf("\n");
    }
    return 0;
}
